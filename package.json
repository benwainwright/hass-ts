{
  "name": "rtm-typescript",
  "version": "1.0.0",
  "type": "module",
  "description": "API Client for Remember The Milk API written in TypeScript",
  "main": "index.js",
  "scripts": {
    "extract-api": "vite-node ./api/extract.ts",
    "build": "tsc --project tsconfig.dist.json && npm run extract-api",
    "lint": "eslint .",
    "types": "tsc --noEmit",
    "test": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --watch --ui"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@vitest/coverage-istanbul": "0.34.6",
    "@vitest/ui": "0.34.6",
    "msw": "2.0.1",
    "prettier": "3.0.3",
    "typescript": "5.2.2",
    "vite-node": "^0.34.6",
    "vitest": "0.34.6"
  },
  "dependencies": {
    "@microsoft/api-extractor": "^7.38.1"
  }
}
