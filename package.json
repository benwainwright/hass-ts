{
  "name": "rtm-typescript",
  "version": "1.0.0",
  "type": "module",
  "description": "API Client for Remember The Milk API written in TypeScript",
  "main": "index.js",
  "scripts": {
    "api:prod": "vite-node ./api/extract.ts '<projectFolder>/dist/index.d.ts'",
    "api-check": "tsc --project tsconfig.checkapi.json && vite-node ./api/extract.ts '<projectFolder>/tmp-declarations/index.d.ts' && rm -rf tmp-declarations",
    "build": "tsc --project tsconfig.dist.json && npm run api:prod",
    "lint": "eslint .",
    "types": "tsc --noEmit",
    "types:watch": "tsc --noEmit --watch",
    "test": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --watch --ui",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@vitest/coverage-istanbul": "0.34.6",
    "@vitest/ui": "0.34.6",
    "eslint": "^8.52.0",
    "eslint-plugin-filenames": "^1.3.2",
    "husky": "^8.0.3",
    "msw": "2.0.1",
    "prettier": "3.0.3",
    "typescript": "5.2.2",
    "vite-node": "^0.34.6",
    "vitest": "0.34.6"
  },
  "dependencies": {
    "@microsoft/api-extractor": "^7.38.1",
    "@types/jest-when": "^3.5.4",
    "jest-when": "^3.6.0"
  }
}
