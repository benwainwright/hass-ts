{
  "name": "rtm-typescript",
  "version": "1.2.9",
  "files": [
    "dist/index.foo",
    "dist/src/lib",
    "dist/rtm-typescript.d.ts"
  ],
  "description": "Strongly typed SDK client for the Remember the Milk API",
  "main": "dist/index.js",
  "typings": "dist/rtm-typescript.d.ts",
  "author": "Ben Wainwright",
  "license": {
    "type": "MIT",
    "url": "https://www.opensource.org/licenses/mit-license.php"
  },
  "scripts": {
    "api:prod": "vite-node ./api/extract.ts '<projectFolder>/dist/dts/index.d.ts'",
    "api-check": "tsc --project tsconfig.checkapi.json && vite-node ./api/extract.ts '<projectFolder>/tmp-declarations/dts/index.d.ts' && rm -rf tmp-declarations",
    "build": "tsc --project tsconfig.dist.json && npm run api:prod",
    "lint": "eslint .",
    "types": "tsc --noEmit",
    "types:watch": "tsc --noEmit --watch",
    "test": "vitest --config vitest.unit.config.mts --coverage",
    "test:e2e": "vitest --config vitest.e2e.config.mts",
    "test:e2e:post-release": "vitest --config vitest.e2e.post-release.config.mts",
    "test:watch": "vitest --config vitest.unit.config.mts --watch",
    "test:ui": "vitest --config vitest.unit.config.mts --watch --ui",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@microsoft/api-extractor": "^7.38.1",
    "@types/jest-when": "^3.5.4",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@vitest/coverage-istanbul": "0.34.6",
    "@vitest/ui": "0.34.6",
    "eslint": "^8.52.0",
    "eslint-plugin-filenames": "^1.3.2",
    "husky": "^8.0.3",
    "jest-when": "^3.6.0",
    "msw": "2.0.1",
    "prettier": "3.0.3",
    "typescript": "5.2.2",
    "vite-node": "^0.34.6",
    "vitest": "0.34.6",
    "vitest-mock-extended": "^1.3.1"
  }
}
